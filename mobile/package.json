{
	"name": "mobile",
	"version": "1.0.0",
	"description": "Appium mobile test",
	"main": "index.js",
	"directories": {
		"lib": "lib"
	},
	"scripts": {
		"test:app": "jest --forceExit",
		"test:app:debug": "jest src/test/*.test.ts --detectOpenHandles --watch --verbose=false --silent=false",
		"eslinter": "./node_modules/.bin/eslint .",
		"update:eslinter": "node updateeslintrc.js"
	},
	"jest": {
		"testMatch": [
			"src/test/*.test.js",
			"src/test/**/*.test.js"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run eslinter"
		}
	},
	"keywords": [
		"Appium",
		"Android"
	],
	"author": "Carlos Ferrao",
	"license": "ISC",
	"dependencies": {
		"chai": "^4.3.0",
		"chai-as-promised": "^7.1.1",
		"colors": "^1.4.0",
		"dotenv": "^8.2.0",
		"jest": "^26.6.3",
		"jest-html-reporters": "^2.1.2",
		"node-resemble-js": "^0.2.0",
		"png-crop": "^0.0.2",
		"wd": "^1.14.0"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"eslint": "^7.20.0"
	}
}
